#include<stdio.h>
void main(){
	
	
	
	int choice;
	float temp;
	printf("Enter temp in F");
	scanf("%f",&temp);
	printf("F to C (1) or \nC to F (2)");
	scanf("%d",&choice);
	if(choice==1){
		printf("The temperature in C is %0.2f",(temp*1.8)+32);
	}
	else if(choice==2){
		printf("The temperature in C is %0.2f",(temp-32)*0.5);
	}
	else{
		printf("Enter valid input");
	}
	
	
	
	

	int score;
	sum+=printf("Enter score.\n");
	scanf("%d",&score);
	
	if(score>100||score<0){
		printf("invalid\n");
	}
	else if(score>=90){
		printf("S");
	}
	else if(score>=80&&score<=89){
		printf("A");
	}
	else if(score>=70&&score<=79){
		printf("B");
	}
	else if(score>=60&&score<=69){
		printf("C");
	}
	else if(score>=55&&score<=59){
		printf("D");
	}
	else if(score>=50&&score<=54){
		printf("E");
	}
	else{
		printf("F");
	}


	
	
	
	
	int a,b;
	char c;
	printf("Type in your question\n");
	scanf("%d %c %d",&a,&c,&b);
	
	switch(c){
		case '+':
			printf("%d",a+b);
			break;
		case '-':
			printf("%d",a-b);
			break;
		case '*':
			printf("%d",a*b);
			break;
		case '/':
			printf("%d",a/b);
		default:
			printf("Invalid input\n");
	}
	
}

